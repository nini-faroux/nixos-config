{
 "languageserver": {
   "haskell": {
     "command": "haskell-language-server-wrapper",
     "args": ["--lsp"],
     "rootPatterns": [
       "BUILD.bazel",
       ".hie-bios",
       "*.cabal",
       "stack.yaml",
       "cabal.project",
       "package.yaml"
     ],
     "filetypes": [
       "hs",
       "lhs",
       "haskell"
     ],
     "initializationOptions": {},
     "settings": {
       "languageServerHaskell": {
          "hlintOn": true,
          "maxNumberOfProblems": 10,
          "completionSnippetsOn": true,
          "formattingProvider" : "ormolu"
       }
     }
   }
 },
 "rust-analyzer.serverPath": "/nix/store/pwwy6rrm3h18fmqp4nc8w4rscx24pwaw-home-manager-path/bin/rust-analyzer"
}
